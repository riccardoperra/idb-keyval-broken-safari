#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/riccardoperra/WebstormProjects/idb-keyval-broken-safari/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/node_modules:/Users/riccardoperra/WebstormProjects/idb-keyval-broken-safari/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/node_modules:/Users/riccardoperra/WebstormProjects/idb-keyval-broken-safari/node_modules/.pnpm/jsesc@2.5.2/node_modules:/Users/riccardoperra/WebstormProjects/idb-keyval-broken-safari/node_modules/.pnpm/node_modules:/Users/riccardoperra/WebstormProjects/idb-keyval-broken-safari/node_modules:/Users/riccardoperra/WebstormProjects/node_modules:/Users/riccardoperra/node_modules:/Users/node_modules:/node_modules:/Users/riccardoperra/WebstormProjects/idb-keyval-broken-safari/node_modules/.pnpm/@babel+generator@7.18.2/node_modules/@babel/generator/node_modules:/Users/riccardoperra/WebstormProjects/idb-keyval-broken-safari/node_modules/.pnpm/@babel+generator@7.18.2/node_modules/@babel/node_modules:/Users/riccardoperra/WebstormProjects/idb-keyval-broken-safari/node_modules/.pnpm/@babel+generator@7.18.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/riccardoperra/WebstormProjects/idb-keyval-broken-safari/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/node_modules:/Users/riccardoperra/WebstormProjects/idb-keyval-broken-safari/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/node_modules:/Users/riccardoperra/WebstormProjects/idb-keyval-broken-safari/node_modules/.pnpm/jsesc@2.5.2/node_modules:/Users/riccardoperra/WebstormProjects/idb-keyval-broken-safari/node_modules/.pnpm/node_modules:/Users/riccardoperra/WebstormProjects/idb-keyval-broken-safari/node_modules:/Users/riccardoperra/WebstormProjects/node_modules:/Users/riccardoperra/node_modules:/Users/node_modules:/node_modules:/Users/riccardoperra/WebstormProjects/idb-keyval-broken-safari/node_modules/.pnpm/@babel+generator@7.18.2/node_modules/@babel/generator/node_modules:/Users/riccardoperra/WebstormProjects/idb-keyval-broken-safari/node_modules/.pnpm/@babel+generator@7.18.2/node_modules/@babel/node_modules:/Users/riccardoperra/WebstormProjects/idb-keyval-broken-safari/node_modules/.pnpm/@babel+generator@7.18.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../jsesc@2.5.2/node_modules/jsesc/bin/jsesc" "$@"
else
  exec node  "$basedir/../../../../../../jsesc@2.5.2/node_modules/jsesc/bin/jsesc" "$@"
fi
